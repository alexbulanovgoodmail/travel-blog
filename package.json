{
  "name": "vite-starting-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint \"**/*.{js,mjs,jsx,ts,mts,tsx,vue}\" && stylelint \"**/*.{css,scss,vue}\"",
    "lint:styles": "stylelint \"**/*.{css,scss,vue}\"",
    "lint:styles:fix": "stylelint \"**/*.{css,scss,vue}\" --fix",
    "lint:scripts": "eslint \"**/*.{js,mjs,jsx,ts,mts,tsx,vue}\"",
    "lint:scripts:fix": "eslint \"**/*.{js,mjs,jsx,ts,mts,tsx,vue}\" --fix",
    "format": "prettier --check \"**/*.{js,mjs,jsx,ts,mts,tsx,vue,css,scss,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,mjs,jsx,ts,mts,tsx,vue,css,scss,json,md}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "postcss-discard-duplicates": "^7.0.2",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "3.6.2",
    "sass": "^1.93.3",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-html-inject": "^1.1.2",
    "vite-plugin-svg-spriter": "^1.0.0",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  },
  "lint-staged": {
    "*.{css,scss,vue}": "stylelint --fix",
    "*.{js,mjs,jsx,ts,mts,tsx,vue}": "eslint --fix",
    "*.{js,mjs,jsx,ts,mts,tsx,css,scss,vue,json,md}": "prettier --write"
  }
}
